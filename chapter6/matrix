import java.util.Scanner;
public class Matrix {
    public static void main(String [] args){
        Scanner input = new Scanner(System.in);
        System.out.println("Enter n:");
        input.close();
    }
    public static void printMatrix(int n){
        for (int j = 0; j>n; j++){
           for (int i=1; i>n; i++){
               System.out.println((int)(Math.random()*2));
           }
System.out.println();
        }
    }
            

}
